pipeline{
  agent any 
  stages{
    stage('check out'){
      steps{
        git 'https://github.com/Steephanjeyaraj/HelloWorld.git'
        }
    }
    stage('taurus execution'){
      steps{
        bzt 'Google.yml -o settings.artifacts-dir="C:\\Users\\steephan\\Desktop\\BlazeMeter\\jenkiks-${JOB_NAME}-${BUILD_NUMBER}"'
      }
    }
    stage('Result Collation'){
      steps{
        dir('C:\\Users\\steephan\\Desktop\\Blazemeter\\jenkine-${JOB_NAME}-${BUILD_NUMBER}') {
                bat 'java -jar TaurusOutputConverter.jar kpi.jtl jmeterOutput.jtl'
                bat 'jmeter.bat -g jmeterOutput.jtl -o .\\JmeterReport"
         }
      }
    }
  }
}
