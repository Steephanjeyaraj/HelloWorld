pipeline{
  agent any 
  stages{
    stage('check out'){
      steps{
        git 'https://github.com/Steephanjeyaraj/HelloWorld.git'
        }
    }
    stage('taurus execution'){
      steps{
        bzt 'Google.yml -o settings.artifacts-dir="C:\\Users\\steephan\\Desktop\\BlazeMeter\\jenkiks-${JOB_NAME}-${BUILD_NUMBER}"'
      }
    }
    stage('Result Collation'){
      steps{
        bat 'java -jar TaurusOutputConverter.jar %1 %2' C:\\Users\\steephan\\Desktop\\BlazeMeter\\jenkiks-${JOB_NAME}-${BUILD_NUMBER}\\kpi.jtl C:\\Users\\steephan\\Desktop\\BlazeMeter\\jenkiks-${JOB_NAME}-${BUILD_NUMBER}\\jmeterOutput.jtl
        bat 'jmeter.bat -g %1 -o %2' C:\\Users\\steephan\\Desktop\\Blazemeter\\jenkiks-${JOB_NAME}-${BUILD_NUMBER}\\jmeterOutput.jtl C:\\Users\\steephan\\Desktop\\Blazemeter\\jenkiks-${JOB_NAME}-${BUILD_NUMBER}\\JmeterReport
      }
    }
  }
}
